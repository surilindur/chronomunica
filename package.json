    "prepare": "npm run build"
  },
  "devDependencies": {
    "@rdfjs/types": "*",
    "@rubensworks/eslint-config": "^2.0.0",
    "componentsjs-generator": "^3.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0"
  }
}